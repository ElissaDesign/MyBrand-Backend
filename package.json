{
  "name": "mybrand-backend",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "coverageReporters": "json-summary",
    "build": "npx babel src --out-dir  dist",
    "test": "nyc --reporter=html --reporter=text --reporter=json-summary mocha --require @babel/register ./dist/test/test.js --timeout 100000000 --exit",
    "test:cover": "npm run test -- --coverage",
    "test:ci": "CI=TRUE npm run test:cover",
    "start": "node dist/app.js",
    "dev": "nodemon --exec babel-node src/app.js",
    "make-badges": "node_modules/.bin/istanbul-badges-readme",
    "make-badges:ci": "npm run make-badges -- --ci"
  },
  "nyc": {
    "extension": [
      ".js"
    ],
    "reporter": [
      "json-summary"
    ],
    "all": true
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ElissaDesign/MyBrand-Backend.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/ElissaDesign/MyBrand-Backend/issues"
  },
  "homepage": "https://github.com/ElissaDesign/MyBrand-Backend#readme",
  "devDependencies": {
    "@babel/core": "^7.17.9",
    "@babel/node": "^7.16.8",
    "@babel/preset-env": "^7.16.11",
    "@babel/register": "^7.17.7",
    "chai": "^4.3.6",
    "coveralls": "^3.1.1",
    "istanbul-badges-readme": "^1.8.1",
    "mocha": "^9.2.2",
    "mocha-lcov-reporter": "^1.3.0",
    "nodemon": "^2.0.15",
    "nyc": "^15.1.0"
  },
  "dependencies": {
    "@babel/cli": "^7.17.6",
    "@hapi/joi": "^17.1.1",
    "@istanbuljs/esm-loader-hook": "^0.1.2",
    "bcrypt": "^5.0.1",
    "chai-http": "^4.3.0",
    "cloudinary": "^1.29.1",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "dotenv": "^16.0.0",
    "express": "^4.17.3",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^6.2.10",
    "multer": "^1.4.4",
    "supertest": "^6.2.3",
    "swagger-jsdoc": "^6.2.1",
    "swagger-ui-express": "^4.3.0"
  }
}
